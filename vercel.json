{
  "functions": {
    "api/*.php": {
      "runtime": "vercel-php@0.6.0"
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/index.php"
    },
{
      "src": "/wp-includes/js/codemirror/codemirror.min.js",
      "dest": "/public/wp-includes/js/codemirror/codemirror.min.js"
    },
    {
      "src": "/wp-includes/js/codemirror/codemirror.min.css",
      "dest": "/public/wp-includes/js/codemirror/codemirror.min.css"
    }
  ]
}
